option_settings:
    aws:elasticbeanstalk:container:python:
        WSGIPath: AltText.wsgi:application
    aws:elasticbeanstalk:environment:proxy:staticfiles:
        /static: static_deploy
    aws:elasticbeanstalk:application:environment:
        DJANGO_SETTINGS_MODULE: fund.productionSettings

container_commands:
    01_migrate:
        command: "source /var/app/venv/staging-LQM1lest/bin/activate django-admin migrate"
        leader_only: true

    02_collectstatic:
        command: "source /var/app/venv/staging-LQM1lest/bin/activate python /var/app/current/manage.py collectstatic "
        leader_only: true